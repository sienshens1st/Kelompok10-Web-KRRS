            <form action="/mainPage/biodata" method="POST">
                <%- include('partials/messages'); %>
                <h4>Personal</h4>
                <hr width="100%" height="1px" color="maroon" size="3">
                <br>
              <label for="nama">Nama</label>
              <input type="text" id="nama" name="nama" placeholder="Nama" required>
              <label for="nim">NIM</label>
              <input type="text" id="nim" name="nim" placeholder="NIM" required>
              <label for="ttl">Tanggal Lahir</label>
              <input type="date" id="lahir" name="lahir" placeholder="Tanggal lahir" value="2000-08-28" required>
              <label for="ips">IPS Terakhir</label>
              <input type="number" id="ips" name="ips" min="1" max="4" step=".01" placeholder="IPS" required>
              <h4>Contact</h4>
              <hr width="100%" height="1px" color="maroon" size="3">
              <br>
              <label for="alamat">Alamat</label>
              <input type="text" id="alamat" name="alamat" placeholder="Alamat" required>

              <label for="telp">No Telepon</label>
              <input type="text" id="telp" name="telp" placeholder="No telepon" required>  


           
            
