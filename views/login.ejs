    <script>
        jQuery(document).ready(function($) {
        var alterClass = function() {
          var ww = document.body.clientWidth;
          if (ww < 600) {
            $('#container1').removeClass('my-4');
              $('#container1').removeClass('mx-5');
          } else if (ww >= 600) {
            $('#container1').addClass('my-4');
              $('#container1').addClass('mx-5');
          };
        };
        $(window).resize(function(){
          alterClass();
        });
        //Fire it when the page first loads:
        alterClass();
      });
    </script>

