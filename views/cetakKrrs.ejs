    <div class ="container-bawah">
            <div class="site">
                <div class="textOnly">
                    <br></br>
                    <h1 style="text-align:center">Cetak KRRS ONLINE</h1>
                    <h2 style="text-align:center" >Mahasiswa Fakultas Teknologi Informasi</h2>
                    <h2 style="text-align:center" >Tahun Ajaran 2020/2021 Semester Ganjil</h2>
                    <%- include('partials/messages'); %>
                    <br>
                </div> 
            </div>
            <div style="margin-left:7.6vw">
                <div class="nama">
                    <p><strong>Nama &nbsp;&nbsp;: <%= typeof user_name != 'undefined' ? user_name : ''%></strong></p>
                </div>
                <div class="nim">
                    <p><strong>Nim &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;: <%= typeof user_nim != 'undefined' ? user_nim : ''%></strong></p>
                </div>  
            </div>       
           
       

        
                    <table class="center">
                      <tr>
                        <th>Kode</th>
                        <th>Mata Kuliah</th>
                        <th>Hari</th>
                         <th>Jam</th>
                        <th>Beban SKS</th>
                      </tr>
                    
                      <% if(user_matkul){ %>
                        <% user_matkul.forEach(data=>{ %>
                            <% list.forEach(data2=>{ %>
                               <% if(data == data2.matkul){ %>
                                <tr>
                                    <td><%=data2.kode%></td>
                                    <td><%=data2.mataKuliah%></td>
                                    <td><%=data2.hari%></td>
                                     <td><%=data2.jam%></td>
                                    <td><%=data2.beban%></td>
                                    <% user_total_beban +=  parseInt(data2.beban) %>
                                  </tr>
                                <% } %>
                            <% }) %>
                       <% }) %>
                       <tr>
                        <td></td>
                        <td><strong>TOTAL SKS</strong></td>
                        <td></td>
                        <td></td>
                        <td><strong><%= user_total_beban %></strong></td>
                       </tr>
                     
                   <%  } %>

                      
                     
                </table>
          
                <br></br> 
                    <div class="perubahan-container">
                        <div class="perubahan"><%= typeof user_kode != 'undefined' ? "Kode Registrasi : " + user_kode : ''%></div>
                        
                        <div class="kodeRegistrasi"><button><a href="/mainPage/cetak/<%= user_id %>">Ubah KRRS</a></button></div>
                         <!-- <div class="kodeRegistrasi"><%= typeof user_perubahan != 'undefined' ? "Perubahan ke - " + user_perubahan : ''%></div>  -->
                    </div>
        
                <br></br> 

                <div class="xcenter">
                        <button><strong><a href="/mainPage/cetak/<%= user_id %>/preview" target="_blank" style="text-decoration: none; color: black;">Cetak KRRS</a></strong></button>

